/**
 * Insert a fast pre-loader image on the trilithon cover image.
 * Load an embedded gif first, then fetch the real image from data-src.
 */
function tm_theme_trilithon_full_placeholder_image($media) {

	global $conf;

	// Check preload covers is enabled
	if (isset($conf["tm_theme_preload_cover_images"])) {
		if ($conf["tm_theme_preload_cover_images"] == true) {

			$placeholder_image_data = tm_theme_get_data_uri_img_805x453();
			$js = 'var fullimage = document.getElementById("preload-cover-image"); if (fullimage.getAttribute("data-src") != "") { fullimage.setAttribute("src",fullimage.getAttribute("data-src")); fullimage.setAttribute("data-src", ""); }';
			$media = str_replace('<img typeof="foaf:Image" src=', "<img typeof=\"foaf:Image\" id='preload-cover-image', OnLoad='" . $js . "' src='" . $placeholder_image_data . "', data-src=", $media);

		}
	}

	return $media;
}

/**
 * Insert a fast pre-loader image on the trilithon card cover image.
 * Load an embedded gif first, then fetch the real image from data-src.
 */
function tm_theme_trilithon_card_placeholder_image($media, $cover_id) {

	global $conf;
	
	// Check preload covers is enabled
	if (isset($conf["tm_theme_preload_cover_images"])) {
		if ($conf["tm_theme_preload_cover_images"] == true) {

			$placeholder_image_data = tm_theme_get_data_uri_img_400x225();
			$js = 'var fullimage = document.getElementById("' . $cover_id . '"); if (fullimage.getAttribute("data-src") != "") { fullimage.setAttribute("src",fullimage.getAttribute("data-src")); fullimage.setAttribute("data-src", ""); }';
			$media = str_replace('<div class="cover"><img typeof="foaf:Image" src=', "<div class=\"cover\"><img typeof=\"foaf:Image\" id='" . $cover_id . "', OnLoad='" . $js . "' src='" . $placeholder_image_data . "', data-src=", $media);

		}
	}

	return $media;
}

